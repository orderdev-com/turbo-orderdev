{
	"globalEnv": [
		"SUPABASE_AUTH_EXTERNAL_GOOGLE_ID",
		"SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET",
		"SUPABASE_URL",
		"SUPABASE_ANON_KEY",
		"SUPABASE_JWT_SECRET"
	],
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [
				".astro/**", // Astro
				".svelte-kit/**" // SvelteKit
				// "dist/**" // astro
			]
		},
		"lint": {},
		"supa_start": {
			"cache": false
		},
		"supa_stop": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
