{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".astro/**",
        ".svelte-kit/**", 
        ".vercel/**"
      ]
    },
    "supa_start": {
      "cache": false
    },
    "supa_stop": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
